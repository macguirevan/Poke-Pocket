    "import pandas as pd\n",
    "\n",
    "# Read the CSV file\n",
    "csv_file = \"data/cards.csv\"  \n",
    "df = pd.read_csv(csv_file)\n",
    "\n",
    "# Convert to JSON\n",
    "json_file = \"data.json\"\n",
    "df.to_json(json_file, orient=\"records\", indent=4)\n",
    "\n",
    "print(f\"CSV successfully converted to {json_file}\")"
